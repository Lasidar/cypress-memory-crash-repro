{
  "name": "cypress-memory-crash-repo",
  "version": "1.0.0",
  "description": "Repro example for cypress and chrome memory crash",
  "main": "index.js",
  "scripts": {
    "open": "npx cypress open --e2e --browser chrome",
    "test": "docker run -it --rm -v $INIT_CWD/cypress:/e2e/cypress -v $INIT_CWD/cypress.config.ts:/e2e/cypress.config.ts -v $INIT_CWD/package.json:/e2e/package.json -v $INIT_CWD/package-lock.json:/e2e/package-lock.json -v $INIT_CWD/tsconfig.json:/e2e/tsconfig.json -w /e2e cypress/browsers:node-20.9.0-chrome-118.0.5993.88-1-ff-118.0.2-edge-118.0.2088.46-1 -- --browser firefox --spec \"cypress/tests/*.feature\""
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/tests",
    "commonPath": "cypress/tests/common"
  },
  "devDependencies": {
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@testing-library/cypress": "^10.0.1",
    "@types/chance": "^1.1.3",
    "blueimp-md5": "^2.19.0",
    "chance": "^1.1.8",
    "cheerio": "1.0.0-rc.10",
    "cypress": "13.0.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "fs-extra": "^10.1.0",
    "jStat": "^1.8.6",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "numeral": "^2.0.6",
    "numeric": "^1.2.6",
    "typescript": "^4.9.5",
    "underscore.string": "^3.3.6"
  }
}
